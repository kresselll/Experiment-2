//A program that generates the multiplication table (up to x10) of a number.

#include <iostream>
using namespace std;

int main()
{
    int a, b;

    cout << "Enter a number: ";
    cin >> a;
    cout<< "\n\nMultiplication table of:";

    for (b = 1; b <= 10; ++b)
	{
        cout << "\n"<< a << " * " << b << " = " << a * b << endl;
    }
    
    return 0;
}
