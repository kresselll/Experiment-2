//A program that will “count” numbers, separated by commas, up to 10. When it reaches 10, it will count by 2s until 30 is reached.

#include <iostream>
using namespace std;

int main ()
{
	int counter;
	
	
	for (counter=1;counter<=10;counter++)
	cout << counter << ", " ;
	for (counter=12;counter<=30;counter+=2)
	cout << counter << ", " ;

	return 0;
}
